{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/recommended",
    "eslint:recommended",
    "airbnb-base"
  ],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "rules": {
    "import/prefer-default-export": "off",
    "vue/max-attributes-per-line": "off"
  }
}
